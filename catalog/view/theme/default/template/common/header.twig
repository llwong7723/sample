<!DOCTYPE html>
<!--[if IE]><![endif]-->
<!--[if IE 8 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie8"><![endif]-->
<!--[if IE 9 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie9"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html dir="{{ direction }}" lang="{{ lang }}">
<!--<![endif]-->
<head>
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<title>{{ title }}</title>
<base href="{{ base }}" />
{% if description %}
<meta name="description" content="{{ description }}" />
{% endif %}
{% if keywords %}
<meta name="keywords" content="{{ keywords }}" />
{% endif %}
<script src="catalog/view/javascript/jquery/jquery-2.1.1.min.js" type="text/javascript"></script>
<link href="catalog/view/javascript/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen" />
<script src="catalog/view/javascript/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
<link href="catalog/view/javascript/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
<link href="//fonts.googleapis.com/css?family=Open+Sans:400,400i,300,700" rel="stylesheet" type="text/css" />
<link href="catalog/view/theme/default/stylesheet/custom/stylesheet.css" rel="stylesheet">
<link href="catalog/view/theme/default/stylesheet/custom/header.css" rel="stylesheet">
<link href="catalog/view/theme/default/stylesheet/custom/pushy.css" rel="stylesheet">
{% for style in styles %}
<link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
{% endfor %}
{% for script in scripts %}
<script src="{{ script }}" type="text/javascript"></script>
{% endfor %}
<script src="catalog/view/javascript/common.js" type="text/javascript"></script>
{% for link in links %}
<link href="{{ link.href }}" rel="{{ link.rel }}" />
{% endfor %}
{% for analytic in analytics %}
{{ analytic }}
{% endfor %}
</head>
<body>

<!-- Desktop -->
<header class="large hidden-md hidden-sm hidden-xs">
  <div class="d-container">
    <nav>
      <a href="#"><img id="img-logo" class="logo img-responsive" src="image/catalog/default/sample_logo_large_470x235.png"></a>
      <ul class="menu-right">
        <li><a class="active" href="#1">Home</a></li>
        <li><a href="#2">About Us</a></li>
        <li><a href="#3">Menu</a></li>
        <li><a href="#4">Delivery</a></li>
        <li><a href="#5">Contact</a></li>
      </ul>
    </nav>
  </div>
</header>


<!-- Tablet -->
<header class="large hidden-lg hidden-sm hidden-xs">
  <div class="t-container">
    <nav>
      <a href="#"><img id="img-logo-t" class="logo img-responsive" src="image/catalog/default/sample_logo_large_470x235.png"></a>
      <ul class="menu-right">
        <li><a class="active" href="#1">Home</a></li>
        <li><a href="#2">About Us</a></li>
        <li><a href="#3">Menu</a></li>
        <li><a href="#4">Delivery</a></li>
        <li><a href="#5">Contact</a></li>
      </ul>
    </nav>
  </div>
</header>


<!-- Mobile -->
<header class="large visible-sm visible-xs push">
  <div class="m-container">
    <nav>
      <div class="menu-icon">
      <i class="fa fa-bars fa-2x"></i>
      </div>
      <a href="#"><img id="img-logo-m" class="logo-m img-responsive" src="image/catalog/default/sample_logo_large_470x235.png"></a>
    </nav>
  </div>
</header>
<nav class="pushy pushy-right" data-focus="#first-link">
  <div class="pushy-content">
      <ul>
        <li class="pushy-mainmenu"><a class="active" href="#1">Home</a></li>
        <li class="pushy-mainmenu"><a href="#2">About Us</a></li>
        <li class="pushy-mainmenu"><a href="#3">Menu</a></li>
        <li class="pushy-mainmenu"><a href="#4">Delivery</a></li>
        <li class="pushy-mainmenu"><a href="#5">Contact</a></li>
      </ul>
  </div>
</nav>
<!-- Site Overlay -->
<div class="site-overlay"></div>
<script>
$(document).on("scroll", function() {
	if($(document).scrollTop()>100) {
		$("header").removeClass("large").addClass("small");
    $(".menu-icon").css("color","#000000");
    document.getElementById("img-logo").src="image/catalog/default/sample_logo_small_470x235.png";
    document.getElementById("img-logo-t").src="image/catalog/default/sample_logo_small_470x235.png";
    document.getElementById("img-logo-m").src="image/catalog/default/sample_logo_small_470x235.png";
    $('header').removeClass('push');
    $('.bg-mainbanner').removeClass('push');
	} else {
		$("header").removeClass("small").addClass("large");
    $(".menu-icon").css("color","#FFFFFF");
    document.getElementById("img-logo").src="image/catalog/default/sample_logo_large_470x235.png";
    document.getElementById("img-logo-t").src="image/catalog/default/sample_logo_large_470x235.png";
    document.getElementById("img-logo-m").src="image/catalog/default/sample_logo_large_470x235.png";
    $('header').addClass('push');
    $('.bg-mainbanner').addClass('push');
	}
});
</script>
<script src="catalog/view/javascript/custom/pushy.js" type="text/javascript"></script>